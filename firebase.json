{
  "firebase": "hivemind-data",
  "public": "data",
  "ignore": [
    "firebase.json",
    "**/historical/**",
    "**/.*",
    "**/node_modules/**"
  ],
  "headers": [{
    "source": "**/*.json",
    "headers": [{
      "key": "Access-Control-Allow-Origin",
      "value": "*"
    }]
  }, {
    "source": "**/*.json",
    "headers": [{
      "key": "Cache-Control",
      "value": "max-age=0"
    }]
  }]
}
